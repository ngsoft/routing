{
  "name": "ngsoft/routing",
  "description": "Symfony foundation routing with PSR-15 adapters for standalone projects",
  "version": "1.1.2",
  "keywords": [
    "routing",
    "router",
    "middleware",
    "symfony"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Aymeric Anger",
      "role": "Developer",
      "homepage": "https://github.com/ngsoft"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "process-timeout": 0,
    "platform-check": false
  },
  "autoload": {
    "psr-4": {
      "NGSOFT\\": "src/"
    }
  },
  "suggest": {},
  "provide": {},
  "require": {
    "php": ">=8.2",
    "guzzlehttp/psr7": "^2",
    "ngsoft/container": "^3",
    "nikic/fast-route": "^1",
    "psr/http-server-handler": "^1",
    "psr/http-server-middleware": "^1",
    "reindeer/symfony-middleware": "^1",
    "symfony/http-foundation": "^6.4|^7",
    "symfony/http-kernel": "^6.4|^7",
    "symfony/psr-http-message-bridge": "^6.4|^7"
  },
  "require-dev": {
    "symfony/var-dumper": "^6.4|^7",
    "friendsofphp/php-cs-fixer": "^3",
    "jetbrains/phpstorm-attributes": "dev-master"
  },
  "scripts": {
    "serve": [
      "Composer\\Config::disableProcessTimeout",
      "php -S 127.0.0.1:8123 -t public"
    ]
  }
}
